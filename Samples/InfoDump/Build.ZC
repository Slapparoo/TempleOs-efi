Cd(__DIR__);

#define project_name "InfoDump"

U0 Clean() {
	Del(project_name ".ZXE");
    Del(project_name ".efi");
}

U0 Build(Bool cleanup=TRUE) {
	// Standard HolyC Compile produces .ZXE
	I64 errorCount = Comp(project_name);

	// Produce .efi file from the .ZXE 
	if (!errorCount) {
		EfiFromBin(project_name);
		if (cleanup) {
			Del(project_name ".ZXE");
		}
    }
}

Clean;
Build;